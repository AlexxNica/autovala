#: ../src/autovala_gedit.vala:40 ../data/interface/ask_to_continue.ui.h:1
msgid "Delete 'install' folder"
msgstr "Borrar el directorio 'folder'"

#: ../src/autovala_gedit.vala:42
msgid "All the content of the 'install' folder will be deleted. Continue?"
msgstr "Se borrará todo el contenido de la carpeta 'install'. ¿Continuar?"

#: ../src/autovala_gedit.vala:82
msgid "Autovala"
msgstr "Autovala"

#: ../src/autovala_gedit.vala:84
msgid "Open project file"
msgstr "Abrir fichero de proyecto"

#: ../src/autovala_gedit.vala:84
msgid "Opens the project file for the current source file"
msgstr "Abre el fichero de proyecto asociado al fichero fuente actual"

#: ../src/autovala_gedit.vala:87
msgid "Update project"
msgstr "Actualizar el proyecto"

#: ../src/autovala_gedit.vala:87
msgid "Updates the .avprj project file and CMakeLists files"
msgstr "Actualiza el fichero de proyecto .avprj y los ficheros CMakeLists"

#: ../src/autovala_gedit.vala:90
msgid "Run CMake"
msgstr "Ejecutar CMake"

#: ../src/autovala_gedit.vala:90
msgid "Runs CMake to create the Makefile"
msgstr "Ejecuta CMake para crear los ficheros Makefile"

#: ../src/autovala_gedit.vala:93
msgid "Build project"
msgstr "Construir proyecto"

#: ../src/autovala_gedit.vala:93
msgid "Builds the project running MAKE"
msgstr "Construye el fichero con MAKE"

#: ../src/autovala_gedit.vala:96
msgid "Make clean"
msgstr "Make clean"

#: ../src/autovala_gedit.vala:96
msgid "Cleans the project folder with 'make clean'"
msgstr "Limpia la carpeta del proyecto con 'make clean'"

#: ../src/autovala_gedit.vala:99
msgid "Full clean"
msgstr "Clean completo"

#: ../src/autovala_gedit.vala:99
msgid "Deletes the 'install' folder, to recreate the cmake system from scratch"
msgstr ""
"Borra el directorio 'install', para recrear el sistema cmake desde cero"

#: ../src/autovala_gedit.vala:102
msgid "Full build"
msgstr "Construcción completa"

#: ../src/autovala_gedit.vala:102
msgid ""
"Deletes the 'install' folder, updates the Autovala .avprj project file, runs "
"cmake and launches make for a full build"
msgstr ""
"Borra el directorio 'install', actualiza el fichero .avprj, ejecuta cmake y "
"lanza make para una compilación completa"

#: ../src/autovala_gedit.vala:183
#, c-format
msgid "Launching '%s' at folder '%s'\n"
msgstr "Lanzando '%s' desde el directorio '%s'\n"

#: ../src/autovala_gedit.vala:195
msgid "Failed\n"
msgstr "Fallo\n"

#: ../src/autovala_gedit.vala:214
#, c-format
msgid "Done (return value %d)\n"
msgstr "Hecho (valor de retorno %d)\n"

#: ../src/autovala_gedit.vala:265
msgid "Updating project and cmake files\n"
msgstr "Actualizando ficheros de proyecto y de CMake\n"

#: ../src/autovala_gedit.vala:270 ../src/autovala_gedit.vala:276
msgid "Aborting\n"
msgstr "Abortando\n"

#: ../src/autovala_gedit.vala:272
msgid "Updating CMake files\n"
msgstr "Actualizando los ficheros de CMake\n"

#: ../src/autovala_gedit.vala:278
msgid "Done\n"
msgstr "Hecho\n"

#: ../src/autovala_gedit.vala:286
msgid "Failed to get access to the project file\n"
msgstr "No se ha podido acceder al fichero de proyecto\n"

#~ msgid ""
#~ "Autovala. Usage:\n"
#~ "\tautovala help: shows this help\n"
#~ "\tautovala version: shows the current version\n"
#~ "\tautovala init project_name: initializates a new Vala CMake project and "
#~ "creates an initial project file\n"
#~ "\tautovala refresh: tries to guess the type for each file in the folders "
#~ "and adds them to the project file\n"
#~ "\tautovala cmake: creates the CMake files from the project file\n"
#~ "\tautovala update: the same than 'refresh'+'cmake'\n"
#~ "\tautovala clear: removes the automatic parts in the project file, "
#~ "leaving only the manual ones.\n"
#~ "\n"
#~ msgstr ""
#~ "Autovala: uso:\n"
#~ "\tautovala help: muestra esta ayuda\n"
#~ "\tautovala version: muestra la versión actual\n"
#~ "\tautovala init project_name: inicializa Vala y CMake, y crea un fichero "
#~ "de proyecto inicial\n"
#~ "\tautovala refresh: intenta adivinar el tipo de cada fichero en las "
#~ "carpetas y los añade al fichero de proyecto\n"
#~ "\tautovala cmake: crea los ficheros para CMake a partir del fichero de "
#~ "proyecto\n"
#~ "\tautovala update: mismo resultado que 'refresh'+'cmake'\n"
#~ "\tautovala clear: elimina las partes automáticas en el fichero de "
#~ "proyecto, dejando sólo las añadidas manualmente.\n"
#~ "\n"

